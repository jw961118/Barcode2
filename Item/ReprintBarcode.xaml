<Window x:Class="Item.ReprintBarcode"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ReprintBarcode" Height="300" Width="300" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Barcode Number:" Height="25" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" Width="108" />
        <TextBox Height="22" HorizontalAlignment="Left" Margin="16,40,0,0" Name="txtBox_PrintBarcode" VerticalAlignment="Top" Width="248" />
        <Button Content="Print" Height="27" HorizontalAlignment="Left" Margin="190,72,0,0" Name="btn_Print" VerticalAlignment="Top" Width="74" Click="btn_Print_Click" />
    </Grid>
</Window>
