        <Label Content="Desc:" Height="24" HorizontalAlignment="Left" Margin="30,58,0,0" Name="label9" VerticalAlignment="Top" Width="46" />
        <Label Content="Delete Item" FontWeight="Bold" Height="24" HorizontalAlignment="Left" Margin="10,10,0,0" Name="label10" VerticalAlignment="Top" Width="90" />
