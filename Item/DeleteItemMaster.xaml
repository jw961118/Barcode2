<Window x:Class="Item.DeleteItemMaster"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DeleteItemMaster" Height="300" Width="300" WindowStartupLocation="CenterScreen" Icon="D:\Task 4\Item2\img\delete.ico">
    <Grid>
